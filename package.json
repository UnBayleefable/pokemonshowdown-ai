{
  "name": "pokemonshowdown-ai",
  "version": "0.0.1",
  "description": "AI project for Pokemon Showdown",
  "private": true,
  "main": "dist/psbot/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "train": "node -r source-map-support/register dist/train/index.js",
    "train:gpu": "npm run train -- --gpu",
    "psbot": "node -r source-map-support/register dist/psbot/index.js",
    "psbot:gpu": "npm run psbot -- --gpu",
    "lint": "tslint -c tslint.json -e src/**/dex.ts --project .",
    "pretest": "npm run lint",
    "pretest:nocov": "npm run pretest",
    "test": "npm run test:unit",
    "test:nocov": "npm run test:unit:nocov",
    "test:unit": "nyc mocha --spec test/unit.ts",
    "test:unit:nocov": "mocha --spec test/unit.ts"
  },
  "mocha": {
    "file": "test/setup-chai.ts",
    "recursive": true,
    "require": [
      "source-map-support/register",
      "ts-node/register"
    ],
    "trace-warnings": true,
    "watch-files": [
      "src/**/*.test.ts"
    ]
  },
  "nyc": {
    "all": true,
    "exclude": [
      "coverage/",
      "dist/",
      "pokemon-showdown/",
      "scripts/",
      "src/config.example.ts",
      "src/config.ts",
      "src/psbot/index.ts",
      "src/train/index.ts",
      "src/**/*.test.ts",
      "src/**/*.js",
      "test/"
    ],
    "reporter": [
      "html",
      "lcov",
      "text"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taylorhansen/pokemonshowdown-ai"
  },
  "keywords": [
    "pokemon"
  ],
  "author": "Taylor Hansen (taylorhansen)",
  "license": "MIT",
  "dependencies": {
    "@pkmn/protocol": "^0.3.4",
    "@pkmn/randoms": "^0.4.11",
    "@pkmn/sim": "^0.4.11",
    "@tensorflow/tfjs": "^3.7.0",
    "@tensorflow/tfjs-node": "^3.7.0",
    "@tensorflow/tfjs-node-gpu": "^3.7.0",
    "long": "^4.0.0",
    "node-fetch": "^2.6.1",
    "progress": "^2.0.3",
    "tfrecord": "^0.2.0",
    "tiny-typed-emitter": "^2.0.3",
    "tmp-promise": "^3.0.2",
    "websocket": "^1.0.34"
  },
  "devDependencies": {
    "@pkmn/data": "^0.4.11",
    "@pkmn/dex": "^0.4.11",
    "@pkmn/dex-types": "^0.4.11",
    "@types/chai": "^4.2.21",
    "@types/chai-as-promised": "^7.1.4",
    "@types/long": "^4.0.1",
    "@types/mocha": "^8.2.3",
    "@types/node": "^16.3.2",
    "@types/node-fetch": "^2.5.11",
    "@types/progress": "^2.0.4",
    "@types/websocket": "^1.0.3",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "mocha": "^9.0.2",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^10.1.0",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  }
}
